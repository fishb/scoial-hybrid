<view class="container address data-v-5d49de73"><hybrid-title class="address-title data-v-5d49de73" vue-id="1" hasTitle="{{true}}" hasStatus="{{true}}" titleHeight="{{titleHeight}}" contentTitle="{{contentTitle}}" leftIcon="{{['icon-back']}}" data-event-opts="{{[['^iconBack',[['$navTo',['']]]]]}}" bind:iconBack="__e" bind:__l="__l" vue-slots="{{['right']}}"><view class="address-title-dot data-v-5d49de73" slot="right"><block wx:if="{{options}}"><text data-event-opts="{{[['tap',[['_AddressDelete',['$0'],['options']]]]]}}" bindtap="__e" class="data-v-5d49de73">删除</text></block></view></hybrid-title><view class="address-detail data-v-5d49de73" style="{{'margin-top:'+(contentMarin)+';'}}"><view class="address-detail-item data-v-5d49de73"><text class="data-v-5d49de73">收货人:</text><input type="text" placeholder="请使用真实姓名" data-event-opts="{{[['input',[['__set_model',['$0','username','$event',[]],['formData']]]]]}}" value="{{formData.username}}" bindinput="__e" class="data-v-5d49de73"/></view><view class="address-detail-item data-v-5d49de73"><text class="data-v-5d49de73">手机号码:</text><input type="number" placeholder="请输入手机号码" data-event-opts="{{[['input',[['__set_model',['$0','tel','$event',[]],['formData']]]]]}}" value="{{formData.tel}}" bindinput="__e" class="data-v-5d49de73"/></view><view data-event-opts="{{[['tap',[['showMulLinkageThreePicker',['$event']]]]]}}" class="address-detail-item data-v-5d49de73" bindtap="__e"><text class="data-v-5d49de73">所在地区:</text><view class="data-v-5d49de73">{{pickerText}}</view></view><view class="address-detail-item big data-v-5d49de73"><text class="data-v-5d49de73">详细地址:</text><textarea type="text" placeholder="请输入详细地址" data-event-opts="{{[['input',[['__set_model',['$0','address','$event',[]],['formData']]]]]}}" value="{{formData.address}}" bindinput="__e" class="data-v-5d49de73"></textarea></view></view><view class="address-switch data-v-5d49de73"><text class="data-v-5d49de73">设置为默认地址<text class="switch-text data-v-5d49de73">(注:每次下单时都会使用该地址)</text></text><switch color="#010101" checked="{{formData.is_default}}" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e" class="data-v-5d49de73"></switch></view><button class="social-button data-v-5d49de73" disabled="{{disabled}}" data-event-opts="{{[['tap',[['_AddressSave']]]]}}" bindtap="__e">保存</button><mpvue-city-picker vue-id="2" themeColor="{{themeColor}}" pickerValueDefault="{{cityPickerValueDefault}}" data-ref="mpvueCityPicker" data-event-opts="{{[['^onCancel',[['onCancel']]],['^onConfirm',[['onConfirm']]]]}}" bind:onCancel="__e" bind:onConfirm="__e" class="data-v-5d49de73 vue-ref" bind:__l="__l"></mpvue-city-picker></view>